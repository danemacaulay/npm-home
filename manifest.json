{
  "name": "npm home",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Redirect to a package's github page. Simulates original behaviour of `npm home`.",
  "content_scripts": [{
    "matches": ["https://npmjs.org/package/*"],
    "run_at": "document_end",
    "js": ["index.js"],
    "css": ["style.css"]
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["tabs", "https://npmjs.org/package/*"]
}
